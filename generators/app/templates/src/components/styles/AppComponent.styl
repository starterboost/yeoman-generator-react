.container
	
	.content
		padding 10px
	

	ul.menu
		list-style none
		margin 0px
		padding 0px
		
		li
			float left
			list-style none
			margin 0px 10px 0px 0px
			padding 0px
		
		&::after
			content ''
			display block
			clear both
		
	

	.logout
		position absolute
		top 0
		right 0
		z-index 100
	

